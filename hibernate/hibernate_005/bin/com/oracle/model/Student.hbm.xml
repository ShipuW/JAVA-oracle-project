<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.oracle.model.Student" table="STUDENT">
        <id name="id" type="java.lang.String">
            <column name="ID" />
            <generator class="uuid" />
        </id>
        <property name="name" type="java.lang.String">
            <column name="NAME" />
        </property>
        <one-to-one name="idCarid" class="com.oracle.model.IdCard" cascade="all" constrained="true" fetch="select"></one-to-one>
       
        <many-to-one name="team" class="com.oracle.model.Team" fetch="select">
        
         <column name="team_id" />
        </many-to-one>
    </class>
</hibernate-mapping>
